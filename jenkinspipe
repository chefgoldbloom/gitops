pipeline {
  agent any

  stages {
    stage('build') {
      steps {
        git branch: 'main',
          url: 'https://github.com/chefgoldbloom/containers'
        sh "docker build -t alpmap alpmap"
      }
    }
    stage('test') {
      steps {
        sh "docker images"
      }
    }
    stage('release') {
      steps {
        sh "docker run alpmap"
      }
    }
}
